ðŸ“„ Paste this:

yaml
Copy
Edit
name: Google Voice Login Automation

on:
  workflow_dispatch:
  push:
    paths:
      - 'Test/forms/Google_Voice_Login_client_00/**'

jobs:
  run-bot:
    runs-on: ubuntu-latest

    defaults:
      run:
        working-directory: Test/forms/Google_Voice_Login_client_00

    steps:
      - name: Checkout repository
        uses: actions/checkout@v3

      - name: Set up Node.js
        uses: actions/setup-node@v3
        with:
          node-version: '20'

      - name: Install dependencies
        run: npm install

      - name: Run login bot
        run: node loginBot.js

      - name: Upload screenshot
        uses: actions/upload-artifact@v3
        with:
          name: login-screenshot
          path: login_success.png
